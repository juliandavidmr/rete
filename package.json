{
  "name": "rete",
  "version": "1.0.0-beta.13",
  "description": "JavaScript framework",
  "main": "build/rete.min.js",
  "types": "build/src/index.d.ts",
  "scripts": {
    "build": "rollup -c && npm run compress",
    "watch": "rollup -cw",
    "lint": "eslint src --fix",
    "compress": "terser --compress -o build/rete.min.js -- build/rete.min.js",
    "build:prod": "rete --build rete.config.js",
    "build:dev": "rete --build rete.config.js --watch",
    "prepublishOnly": "npm run lint && npm run build",
    "test": "BABEL_ENV=test mocha --compilers js:babel-core/register",
    "test:ms": "set BABEL_ENV=test && mocha --compilers js:babel-core/register --require babel-polyfill"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/retejs/rete.git"
  },
  "files": [
    "build/"
  ],
  "keywords": [
    "dataflow",
    "visual programming",
    "node editor",
    "js"
  ],
  "author": "Ni55aN",
  "contributors": [
    {
      "email": "anlijudavid@hotmail.com",
      "name": "anlijudavid",
      "url": "https://github.com/juliandavidmr"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/retejs/rete/issues"
  },
  "homepage": "https://github.com/retejs/rete#readme",
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "eslint": "^5.8.0",
    "jsdom": "^13.0.0",
    "mocha": "^5.2.0",
    "rete-cli": "^0.2.0",
    "rollup": "^0.66.6",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-typescript2": "^0.17.2",
    "terser": "^3.10.10",
    "typescript": "^3.1.6"
  },
  "babel": {
    "presets": [
      "env"
    ]
  }
}
